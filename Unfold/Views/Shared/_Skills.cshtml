@{
    var b = (List<DataBaseEntity.DAO.SkillsTblDAO>)ViewData["skills"];
}

<div class="unslate_co--section section-counter" id="skills-section">
    <div class="container">
        <div class="section-heading-wrap text-center mb-5">
            <h2 class="heading-h2 text-center divider"><span class="gsap-reveal">Yeteneklerim</span></h2>
            <span class="gsap-reveal"><img src="~/unfold-master/images/divider.png" alt="divider" width="76"></span>
        </div>


        <div class="row pt-5">
            @foreach (var skills in b)
            {
                <div class="col-6 col-sm-6 mb-5 mb-lg-0 col-md-6 col-lg-3" data-aos="fade-up" data-aos-delay="0">
                    <div class="counter-v1 text-center">
                        <span class="number-wrap">
                            <span class="number number-counter" data-number="@skills.Skills_Percent">0</span>
                            <span class="append-text">%</span>
                        </span>
                        <span class="counter-label">@skills.Skills_Title</span>
                    </div>
                </div>
            }
                

            </div>
    </div>
</div>