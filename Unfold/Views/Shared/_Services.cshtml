@{
    var b = (List<DataBaseEntity.DAO.ServicesTblDAO>)ViewData["services"];
}
<div class="unslate_co--section" id="services-section">
    <div class="container">

        <div class="section-heading-wrap text-center mb-5">
            <h2 class="heading-h2 text-center divider"><span class="gsap-reveal"> Hizmetlerim </span></h2>
            <span class="gsap-reveal"><img src="~/unfold-master/images/divider.png" alt="divider" width="76"></span>
        </div>

        <div class="row gutter-v3">

            @foreach (var services in b)
            {
                <div class="col-md-6 col-lg-4 mb-4">
                    <div class="feature-v1" data-aos="fade-up" data-aos-delay="0">
                        <div class="wrap-icon mb-3">
                            <img src="~/unfold-master/images/svg/001-options.svg" alt="Image" width="45">
                        </div>
                        <h3> @services.Services_Title </h3>
                        <br/>
                        <p> @services.Services_Content </p>
                    </div>
                </div>
            }



            </div>
    </div>
</div>